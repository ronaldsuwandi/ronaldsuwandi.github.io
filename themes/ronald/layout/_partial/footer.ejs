<footer class="l-main-footer main-footer align-center">
  <ul class="l-social social align-center">
    <li>
      <a href="#" class="l-social-item social-item fa-stack fa-lg social-facebook">
        <i class="fa fa-circle fa-stack-2x"></i>
        <i class="fa fa-facebook fa-stack-1x fa-inverse"></i>
      </a>
    </li>
    <li>
      <a href="#" class="l-social-item social-item fa-stack fa-lg social-twitter">
        <i class="fa fa-circle fa-stack-2x"></i>
        <i class="fa fa-twitter fa-stack-1x fa-inverse"></i>
      </a>
    </li>
    <li>
      <a href="#" class="l-social-item social-item fa-stack fa-lg social-linkedin">
        <i class="fa fa-circle fa-stack-2x"></i>
        <i class="fa fa-linkedin fa-stack-1x fa-inverse"></i>
      </a>
    </li>
    <li>
      <a href="#" class="l-social-item social-item fa-stack fa-lg social-github">
        <i class="fa fa-circle fa-stack-2x"></i>
        <i class="fa fa-github fa-stack-1x fa-inverse"></i>
      </a>
    </li>
    <li>
      <a href="#" class="l-social-item social-item fa-stack fa-lg social-email">
        <i class="fa fa-circle fa-stack-2x"></i>
        <i class="fa fa-envelope fa-stack-1x fa-inverse"></i>
      </a>
    </li>
  </ul>

  <div class="l-copyright copyright">
    <% if (config.author){ %>
    &copy; <%= new Date().getFullYear() %> <%= config.author %>
    <% } else { %>
    &copy; <%= new Date().getFullYear() %> <%= config.title %>
    <% } %>
  </div>
</footer>
